@model KnapSack.Models.Medicament

<div class="form-group">
    @Html.LabelFor(m => m.effetAttendu, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(m => m.effetAttendu, new { htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(m => m.effetAttendu, "", new { @class = "text-danger" })
    </div>
</div>
<div class="form-group">
    @Html.LabelFor(m => m.dureeEffet, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10">
        @Html.EditorFor(m => m.dureeEffet, new { htmlAttributes = new { @class = "form-control", @Value = 1 } })
        @Html.ValidationMessageFor(m => m.dureeEffet, "", new { @class = "text-danger" })
    </div>
</div>